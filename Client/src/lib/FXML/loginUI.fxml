<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane fx:id="root" prefHeight="300.0" prefWidth="500.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="lib.Controller.loginUIController">
   <children>
      <ImageView fitHeight="347.0" fitWidth="513.0" layoutY="-9.0">
         <image>
            <Image url="@../../main/bg.png" />
         </image>
      </ImageView>
      <Button fx:id="loginBtn" layoutX="170.0" layoutY="246.0" mnemonicParsing="false" onAction="#loginButtonAction" prefHeight="23.0" prefWidth="78.0" text="登录！" textAlignment="CENTER" />
      <Button fx:id="registerBtn" layoutX="251.0" layoutY="246.0" mnemonicParsing="false" onAction="#registerButtonAction" prefHeight="23.0" prefWidth="78.0" text="注册" />
      <TextField fx:id="username" layoutX="170.0" layoutY="172.0" />
      <PasswordField fx:id="password" layoutX="170.0" layoutY="208.0" />
      <Label layoutX="120.0" layoutY="174.0" text="用户名" textFill="WHITE">
         <font>
            <Font name="System Bold" size="15.0" />
         </font>
      </Label>
      <Label layoutX="123.0" layoutY="210.0" text="密  码" textFill="WHITE">
         <font>
            <Font name="System Bold" size="15.0" />
         </font>
      </Label>
      <Label fx:id="exit" layoutX="260.0" layoutY="286.0" text="©arttnba3    github.com/arttnba3    All Rights Reserved" textFill="WHITE">
         <font>
            <Font size="9.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
